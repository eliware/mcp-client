{
  "name": "@eliware/mcp-client",
  "type": "module",
  "version": "1.1.0",
  "description": "A Node.js mcp-client.",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "index.mjs",
    "index.cjs",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliware/mcp-client.git"
  },
    "keywords": [
        "mcp-client",
        "nodejs"
    ],
  "author": "Eli Sterling, eliware.org <eli@eliware.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliware/mcp-client/issues"
  },
  "homepage": "https://github.com/eliware/mcp-client#readme",
  "devDependencies": {
    "jest": "^30.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).mjs",
      "**/?(*.)+(spec|test).cjs"
    ]
  },
  "dependencies": {
    "@eliware/common": "^1.1.1"
  }
}
